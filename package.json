{
  "name": "@dabapps/redux-api-collections",
  "version": "0.2.3",
  "description": "Type-safe helpers for dealing with Rest-Framework backed collections in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dist": "./scripts/dist",
    "lint-js": "tslint --project tsconfig.json '@(src|tests|types|docs)/**/*.@(ts|tsx)'",
    "tests": "jest",
    "test": "npm run prettier-check && npm run lint-js && npm run tests -- --coverage",
    "prettier": "prettier --write \"{src,tests}/**/*.{ts,tsx,js,jsx}\"",
    "prettier-check": "./scripts/prettier-check",
    "prepublish": "npm test && npm run dist"
  },
  "dependencies": {
    "@types/js-cookie": "2.0.28",
    "@types/node": "7.0.18",
    "@types/underscore": "1.8.0",
    "axios": "0.16.2",
    "flux-standard-action": "1.2.0",
    "immutable": "3.8.2",
    "js-cookie": "2.1.4",
    "path": "0.12.7",
    "path-to-regexp": "2.1.0",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "typescript": "2.6.2",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "@types/jest": "19.2.3",
    "browserify": "14.4.0",
    "concurrently": "3.5.0",
    "http-server": "0.10.0",
    "jest": "20.0.4",
    "prettier": "1.7.4",
    "ts-jest": "20.0.7",
    "tsify": "3.0.1",
    "tslint": "5.8.0",
    "tslint-config-dabapps": "dabapps/tslint-config-dabapps#v0.4.0",
    "tslint-config-prettier": "1.3.0",
    "uglify-js": "3.0.25",
    "watchify": "3.9.0"
  },
  "peerDependencies": {
    "axios": ">= 0.16.2 < 0.17",
    "typescript": ">= 2.3.3 < 3",
    "immutable": ">= 3.8.2 < 5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dabapps/redux-api-collections.git"
  },
  "author": "DabApps",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/dabapps/redux-api-collections/issues"
  },
  "homepage": "https://github.com/dabapps/redux-api-collections#readme",
  "keywords": [
    "collections",
    "redux",
    "state"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/ts/**/*.{ts,tsx,js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "types\\.(tsx?|jsx?)$"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "mapCoverage": true
  }
}
